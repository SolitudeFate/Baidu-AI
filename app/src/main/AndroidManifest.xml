<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baiduai" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/spiderman_icon"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/spiderman_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.BaiduAI" >

        <activity
            android:name=".StyleConversion"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ColorEnhance"
            android:exported="true" >
        </activity>

        <activity
            android:name=".ContrastEnhance"
            android:exported="true" >
        </activity>

        <activity
            android:name=".ImageColourize"
            android:exported="true" >
        </activity>

        <activity
            android:name=".ImageDefinitionEnhance"
            android:exported="true" >
        </activity>

        <activity
            android:name=".ImageDehazing"
            android:exported="true" >
        </activity>

        <activity
            android:name=".ImageDenoise"
            android:exported="true" >
        </activity>

        <activity
            android:name=".FaceDetection"
            android:exported="true" >
        </activity>

    </application>

</manifest>